<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net481;net9.0</TargetFrameworks>
		<LangVersion>latest</LangVersion>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<EnableWindowsTargeting>true</EnableWindowsTargeting>
		<ProjectGuid>{6692957C-4E2B-415B-A03A-4B3DBCAC071B}</ProjectGuid>
		<OutputType>Library</OutputType>
		<AppDesignerFolder>Properties</AppDesignerFolder>
		<RootNamespace>Tasler</RootNamespace>
		<AssemblyName>Tasler.Core</AssemblyName>
		<DocumentationFile Condition="'$(Configuration)'=='Release'">$(OutputPath)\$(AssemblyName).xml</DocumentationFile>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
	</PropertyGroup>

	<ItemGroup>
	  <Compile Remove="Collections\Generic\**" />
	  <Compile Remove="Configuration\**" />
	  <EmbeddedResource Remove="Collections\Generic\**" />
	  <EmbeddedResource Remove="Configuration\**" />
	  <None Remove="Collections\Generic\**" />
	  <None Remove="Configuration\**" />
	</ItemGroup>

	<ItemGroup>
	  <Compile Remove="Collections\MultiValueDictionary.cs" />
	  <Compile Remove="Collections\MultiValueDictionary.ValueList.cs" />
	  <Compile Remove="EventSubscriber.cs" />
	  <Compile Remove="EventSubscriberDictionary.cs" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="System.Memory" Version="4.6.0" />
	</ItemGroup>

	<ItemGroup>
		<!--<ProjectReference Include="..\Tasler.AssemblyInfo\Tasler.AssemblyInfo.Shared.projitems" />-->
		<ProjectReference Include="..\Tasler.AssemblyInfo\Tasler.AssemblyInfo.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="Properties\Resources.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>Resources.resx</DependentUpon>
		</Compile>
	</ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
	
